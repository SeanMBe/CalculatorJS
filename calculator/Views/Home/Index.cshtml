@{
    ViewBag.Title = "Home Page";
}
<script src="/scripts/knockout-2.2.0.js" ></script>


<div class='liveExample'> 
     
    <div id="result" data-bind='text: result'></div>
    <div>
        <button id="7" data-bind='click: clickSeven'>7</button>
        <button id="8" data-bind='click: clickEight'>8</button>
        <button id="9" data-bind='click: clickNine'>9</button>
    </div>
    <div>
        <button id="4" data-bind='click: clickFour'>4</button>
        <button id="5" data-bind='click: clickFive'>5</button>
        <button id="6" data-bind='click: clickSix'>6</button>
    </div>
    <div>
        <button id="1" data-bind='click: clickOne'>1</button>
        <button id="2" data-bind='click: clickTwo'>2</button>
        <button id="3" data-bind='click: clickThree'>3</button>
    </div>
    
    <button id="0" data-bind='click: clickZero'>0</button>
    <button id="add" data-bind='click: clickAdd'>+</button>
    <button id="equals" data-bind='click: clickEquals'>=</button>

        
</div>​

<script src="/scripts/CalculatorViewModel.js" type="text/javascript"></script>
